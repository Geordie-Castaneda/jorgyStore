<header id="header" class="top-head">
    <!-- Static navbar -->
    <nav class="navbar navbar-default">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-4 col-sm-12 left-rs">
                    <div class="navbar-header">
                        <button type="button" id="top-menu" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a href="{{ path('app_homepage') }}" class="navbar-brand"><img src="{{ asset('images/logo.png') }}" alt="" /></a>
                    </div>
                </div>
                <div class="col-md-8 col-sm-12">
                    <div class="right-nav">
                        <div class="login-sr">
                            {% if app.user %}
                                <a href="{{ path('app_logout') }}">Bienvenido {{ app.user.email }}</a>
                            {% else %}
                                <div class="login-signup">
                                    <ul>
                                        <li><a href="{{ path('app_login') }}">Iniciar sesion</a></li>
                                        <li><a class="custom-b" href="{{ path('app_register') }}">Registrar</a></li>
                                    </ul>
                                </div>
                            {% endif %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/.container-fluid -->
    </nav>
</header>